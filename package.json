{
  "name": "@x011/umi-antd-hashfilter",
  "version": "0.0.1",
  "description": "React components related to umi and ant design - hashfilter",
  "keywords": [
    "linyupark",
    "zion-umi-antd",
    "umi components",
    "antd components",
    "hash filter"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "jsnext:main": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js"
    }
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement -f cjs,es,modern --compress",
    "prettier": "prettier --parser=typescript --write ./src/**/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write ./src/**/*.{ts,tsx}"
    ]
  },
  "author": "linyupark@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linyupark/zion-umi-antd-hashfilter"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.x",
    "react-dom": "^17.x",
    "mitt": "^2.x",
    "antd": "^4.x",
    "umi": "^3.x",
    "moment": "^2.x",
    "qs": "^6.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.x",
    "@typescript-eslint/parser": "^4.x",
    "antd": "^4.x",
    "eslint": "^7.x",
    "eslint-config-prettier": "^7.x",
    "eslint-plugin-prettier": "^3.x",
    "eslint-plugin-react": "^7.x",
    "husky": "^4.x",
    "lint-staged": "^10.x",
    "microbundle": "^0.x",
    "mitt": "^2.x",
    "prettier": "^2.x",
    "react": "^17.x",
    "react-dom": "^17.x",
    "typescript": "^4.x",
    "umi": "^3.x"
  }
}
